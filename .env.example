# Model paths
CKPT_DIR=./app/model

# Default generation parameters
DEFAULT_NUM_INFERENCE_STEPS=40
DEFAULT_NUM_IMAGES_PER_PROMPT=1
DEFAULT_GUIDANCE_SCALE=3.0
DEFAULT_HEIGHT=480
DEFAULT_WIDTH=704
DEFAULT_NUM_FRAMES=121
DEFAULT_FRAME_RATE=25
